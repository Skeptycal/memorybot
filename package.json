{
  "name": "memorybot",
  "version": "1.0.5",
  "description": "An infobot-inspired Slack bot that remembers things",
  "author": "Ian Langworth <ian@langworth.com>",
  "license": "MIT",
  "keywords": [
    "slack",
    "bot",
    "infobot"
  ],
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "scripts": {
    "test": "mocha --compilers coffee:coffee-script/register",
    "coverage": "mocha --compilers coffee:coffee-script/register --require blanket --reporter mocha-lcov-reporter | coveralls",
    "start": "coffee server.coffee",
    "dev": "nodemon -e coffee,js -x coffee server.coffee"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": ["node_modules", "test"]
    }
  },
  "homepage": "https://statico.github.io/memorybot/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/statico/memorybot.git"
  },
  "bugs": {
    "url": "https://github.com/statico/memorybot/issues"
  },
  "dependencies": {
    "artillery-async": "1.0.2",
    "botkit": "0.4.9",
    "chai": "3.5.0",
    "coffee-script": "1.12.2",
    "dotenv": "4.0.0",
    "graceful-fs": "4.1.11",
    "html-entities": "1.2.0",
    "mocha": "3.2.0",
    "sqlite3": "3.1.8",
    "winston": "2.3.0"
  },
  "devDependencies": {
    "blanket": "1.2.3",
    "coveralls": "2.11.15",
    "mocha-lcov-reporter": "1.2.0"
  }
}
